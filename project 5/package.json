{
  "name": "wardrobe-plus",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start",
    "start": "EXPO_NO_TELEMETRY=1 npx expo start --clear",
    "start:reset": "EXPO_NO_TELEMETRY=1 npx expo start --clear --no-dev --minify",
    "clear": "npx expo start --clear && watchman watch-del-all && rm -rf node_modules/.cache",
    "build:web": "npx expo export --platform web",
    "lint": "npx expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/playfair-display": "^0.2.3",
    "@expo-google-fonts/quicksand": "^0.2.3",
    "@expo-google-fonts/raleway": "^0.2.3",
    "@expo-google-fonts/source-sans-pro": "^0.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/native": "^6.1.9",
    "expo": "~50.0.0",
    "expo-blur": "~12.9.1",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.2",
    "expo-linear-gradient": "~12.7.1",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.0",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "lucide-react-native": "^0.358.0",
    "openai": "^4.87.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "babel-preset-expo": "~10.0.0",
    "typescript": "^5.1.3"
  }
}
